{
    "extends":["airbnb", "pretteir", "plugin:node/recommended"],
    "pluggins": ["prettier"],
    "rules":{
        "prettier/prettier":"error",
        "spaced-comment":"off",
        "no-console":"warn",
        "consistent-return":"off",
        "func-names":"off",
        "object-shorthand":"off",
        "no-process-exit":"off",
        "no-param-reassign":"off",
        "no-return-await":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":["error",{"object":true, "array":false}],
        "no-unused-vars":["error", {"argsIgnorePattern": "req|res|next|val"}],
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "never",
            "imports": "never",
            "exports": "never",
            "functions": "never"
        }]
    }
}
